{
  "name": "@nero/plugin-extensions",
  "version": "0.1.0",
  "module": "webview-src/index.ts",
  "type": "module",
  "private": true,
  "types": "./webview-dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./webview-dist/index.js",
      "types": "./webview-dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "bun run build:js --watch",
    "build:js": "bun build webview-src/index.ts --outdir webview-dist --format esm --target browser",
    "build:types": "tsc -p tsconfig.json",
    "build": "bun run build:js && bun run build:types"
  }
}
